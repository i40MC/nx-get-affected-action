    "@typescript-eslint/parser": "^5.26.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.16.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-jest": "^26.4.5",
    "jest": "^28.1.0",
    "jest-circus": "^28.1.0",
